{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["import"],
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": 0,
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error"
  }
}
